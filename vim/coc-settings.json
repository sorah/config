{
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {}
    },
    "ruby": {
      "command": "ruby-lsp",
      "filetypes": ["ruby"],
